{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "自动登录助手",
  "description": "自动登录助手",
  "browser_action": {

  },
  "permissions": [
	"storage",
	"tabs",
    "http://ulog.eloancn.com/*"
	
  ],
  
  "background": {

  },
  "browser_action": {
        "default_icon": {
            "16": "imgs/logo.png",
            "32": "imgs/logo.png",
            "38": "imgs/logo.png",
            "48": "imgs/logo.png",
            "64": "imgs/logo.png",
            "128": "imgs/logo.png"
        },
        "default_title": "配置",
        "default_popup": "popup.html"
    }
}